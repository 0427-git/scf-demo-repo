{
    "serverless-cloud-function-application": {
        "application": {
            "name": "nodejs-helloworld",
            "description": "this is a nodejs hello world",
            "readme": {
                "file": "/readme.md",
                "content": "this is this application readme info"
            },
            "license": {
                "file": "/license.txt",
                "content": "this is this application license info"
            },
            "author": {
                "name": "tencent cloud serverless team"
            },
            "tags": [
                "python2.7"
            ],
            "version": "1.0.1"
        },
        "functions": {
            "name": "hello world function",
            "description": "nodejs hello world function",
            "handler": "demo.main_handler",
            "memorySize": 128,
            "timeout": 3,
            "runtime": "python2.7",
            "indexFile":"demo.js",
            "codeObject": {
                "codeFile": [
                    "demo.js"
                ]
            }
        }
    }
}
      },
      "English":{
        "name": "Websocket_Delete_Demo",
        "description": "This demo uses API GW and SCF to build websocket. This is delete function",
        "attention": "API GW and SCF together build websocket server",
        "readme": {
            "file": "",  // readme file
            "content": ""  //content of readme
        },
        "license": {
            "file": "",  // license file

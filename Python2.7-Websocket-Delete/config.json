      "input_parameters":{
          "event":{
            "websocket":{
              "action":"closing",
              "secConnectionID":"xawexasdfewezdfsdfeasdfffa=="
            }
          },
          "context":""
        },
        "output_parameters":{
          }
        },
      "download_address":"https://github.com/tencentyun/scf-demo-repo/tree/master/Python2.7-API GW Basic Demo",  //demo的git下载链接
      "tags":[
              "apigw", "Python2.7", "basic"  // 标签统一为英文，可编写多个，建议使用runtime、触发器、场景等关键字，用户可以通过该关键字搜索。前台需要展示，请认真填写，不支持中文
      ],
      "version": "1.0.5"  //版本号，通过版本号标识 demo 升级情况，未修改版本号会导致 demo 不更新至页面
    },
    "functions": {
      "name": "api_gw_basic_demo",  // 函数名称，只支持英文
      "description": "本示例演示 API 网关使用的基础方法，主要展示在集成响应下（通过云函数控制台创建的API网关触发器默认启用集成响应），怎样构造返回值",
      "handler":"api_gw_basic_demo.main_handler",
      "memorySize": 128,  // 运行配置内存
      "timeout": 3,  // 运行超时时间
      "runtime": "Python2.7", //运行环境，用户可以通过该关键字搜索["Python2.7", "Python3.6", "Nodejs6.10", "Java8", "LuaCDN", "NodejsCDN", "Php5", "Php7", "Nodejs8.9", "Go1"]
      "Environment":{
      },      // 可选，用于定义环境变量
      "Events":{
        "Type": "TencentCloud::Serverless::Api",
        "Properties":{
          "Path": "/api_gw_basic_demo",
          "StageName": "release",
          "HttpMethod": "ANY",
          "ServiceName":"SCF_API_SERVICE",
          "Auth":"FALSE" //"FALSE"表示免鉴权，“TRUE”表示需要秘钥对
        }
      },      // 可选，用于定义触发此函数的事件源
      "VpcConfig":{
      },      // 可选， 用于配置云函数访问 VPC 私有网络。
      "codeObject": {
          "codeFile": [
              "api_gw_basic_demo.py"
          ],
          "CodeUri":[     // 代码下载地址，和download_address保持一致
              "https://github.com/tencentyun/scf-demo-repo/tree/master/Python2.7-API GW Basic Demo"
          ]
      }
    }
  }
